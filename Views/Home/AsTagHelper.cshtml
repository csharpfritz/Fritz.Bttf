@using Fritz.Bttf.TagHelpers
@addTagHelper "*, Fritz.Bttf"
@{
	
	// Estimated release date from roadmap of ASP.NET 5 RTM
	var destTime = new DateTime(2016, 3, 30, 11, 0, 0, 0);
	
	var nowTime = DateTime.Now;

	// Date ASP.NET was first released
	var fromTime = new DateTime(2002, 1, 5, 10, 0, 0, 0);
	
}

	<div id="timeCircuits">

		<date-dest date="destTime" type="DateDestType.Destination" />
		
		<div class="row"><br/></div>

		<date-dest date="nowTime" type="DateDestType.Present" />
		
		<div class="row"><br/></div>

		<date-dest date="fromTime" type="DateDestType.LastDeparted" />
		
		<div class="row"><br/></div>


	<!-- END TIME CIRCUITS -->		
	</div>
